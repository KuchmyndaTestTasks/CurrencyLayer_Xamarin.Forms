<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mobile.Views.NavigationPage.NavigationDrawerPage"
             xmlns:pages="clr-namespace:Mobile.Views.NavigationPage"
             xmlns:views="clr-namespace:Mobile.Views;assembly=Mobile.Android">
  <MasterDetailPage.Master>
    <pages:NavigationDrawerContent x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
        <NavigationPage >
            <x:Arguments>
                <views:StartPage/>
            </x:Arguments>
        </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>